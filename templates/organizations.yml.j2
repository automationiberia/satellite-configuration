---
# Set the primary organization manually
# primary_organization: ORGANIZATION_NAME

organizations:
{% for organization in info_list %}
  - name: "{{ organization.name }}"
    label: "{{ organization.label }}"
    description: "{{ organization.description }}"
{% endfor %}

all_organizations:
{% for organization in info_list %}
  - "{{ organization.name }}"
{% endfor %}

