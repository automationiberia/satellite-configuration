---
locations:
{% for location in info_list %}
  - name: "{{ location.name }}"
{% if location.description != None and location.description != "" %}
    description: "{{ location.description | regex_replace('[\\r\\n\\t]+',' ') }}"
{% endif %}
{% if location.parent_name != None %}
    parent: "{{ location.parent_name }}"
{% endif %}
{% endfor %}
